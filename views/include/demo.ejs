<form action="/api/flood" method="post" enctype="multipart/form-data" id="add_flood">
    <div class="new_flood"> 
    <div class="form-group"> <label for="name" class="text-light">Flood Name</label>
    <input type="hidden" name="id" value="">
    <input type="text" name="floodname" value="" placeholder="flood-jan2019" required></div>
    <div class="form-group">
    <label for="start-date" class="text-light">Flood Start Date</label><br>
    <input type="date" name="StartDate" id="startdate" required>
     </div>
     <div class="form-group">
    <label for="end-date" class="text-light">Flood End Date</label><br>
    <input type="date" id="enddate" name="EndDate" required>
    </div>
    <div class="form-group">
    <label for="country" class="text-light">Country</label><br>

    <select id="countries" name="CountryName">
        <option value="Bangladesh">Bangladesh</option>
        <option value="Cambodia">Cambodia</option>
        <option value="India">India</option>
        <option value="Indonesia">Indonesia</option>
        <option value="Japan">Japan</option>
        <option value="Laos">Laos</option>
        <option value="Malaysia">Malaysia</option>
        <option value="Myanmar">Myanmar</option>
        <option value="Philippines">Philippines</option>
        <option value="Srilanka">Srilanka</option>
        <option value="Thailand">Thailand</option>
        <option value="Vietnam">Vietnam</option>
        
    </select>
   </div>
   <div class="form-group"> 
    <label for="satellites" class="text-light">Satellites Sensors</label><br>

    <select id="satellites" name="SatelliteName">
        <option value="Planet">Planet</option>
        <option value="Sentinel-I">Sentinel-I</option>
        <option value="Sentinel-II">Sentinel-II</option>
        <option value="DigitalGlobe">Digital Globe</option>
        <option value="Alos-II">Alos-II</option>
    </select>
     </div>
     <div class="form-group"> <label for="name" class="text-light">Latitude</label>
        <input type="hidden" name="id" value="">
        <input type="text" name="Latitude" value="" placeholder="Latitude"  ></div>
        <div class="form-group"> <label for="name" class="text-light">Longitude</label>
            <input type="hidden" name="id" value="">
            <input type="text" name="Longitude" value="" placeholder="Longitude"  ></div>
     <div class="form-group"> 
        <label class="text-light">Upload a CSV file</label><br>

    <input type="file" name="csv"><br><br>
  <button type="submit" class="btn btn-lg btn-primary">Submit</button> </div>
    </div>
</form>